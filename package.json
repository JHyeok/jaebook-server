      "@types/jsonwebtoken": "^8.3.7",
      "@types/node": "^13.5.0",
      "@typescript-eslint/eslint-plugin": "^2.18.0",
      "@typescript-eslint/parser": "^2.18.0",
      "eslint": "^6.8.0",
      "eslint-config-prettier": "^6.9.0",
      "eslint-plugin-prettier": "^3.1.2",
      "nodemon": "^2.0.2",
      "prettier": "^1.19.1",
      "ts-node": "^8.5.4",
      "typescript": "^3.7.4"
